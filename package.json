{
  "name": "wxdevtools-cli",
  "version": "1.1.3",
  "description": "wechatwebdevtools.app cli",
  "main": "lib/index.js",
  "bin": {
    "wxdevtools": "bin/wxdevtools.js"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.6"
  },
  "files": [
    "bin",
    "lib",
    ".wxdevtoolsrc.yaml",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "NODE_ENV=development jest",
    "lint": "eslint .",
    "prepublish": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:jkin8010/wxdevtools-cli.git"
  },
  "keywords": [
    "wechat",
    "devtools",
    "cli"
  ],
  "author": "jkin8010 <jkin8010@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "jest-cli": "^22.4.2",
    "standard": "^11.0.0"
  },
  "dependencies": {
    "chalk": "^2.3.1",
    "execa": "^0.9.0",
    "find-up": "^2.1.0",
    "fkill": "^5.2.0",
    "js-yaml": "^3.10.0",
    "update-notifier": "^2.3.0",
    "user-home": "^2.0.0",
    "which": "^1.3.0"
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__tests__/(fixtures|helpers)/"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "bugs": {
    "url": "https://github.com/jkin8010/wxdevtools-cli/issues"
  },
  "homepage": "https://github.com/jkin8010/wxdevtools-cli#readme",
  "directories": {
    "lib": "lib"
  }
}
